{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to NPM",
            "port": 9229,
            "protocol": "inspector",
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Jasmin via NPM",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "api-test-dev",
                "debug"
            ],
            "port": 9229
        },
        {
            "name": "Launch Program",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/api/index.ts",
            "outFiles": [
                "${workspaceRoot}/dist/index.js"
            ],
            "sourceMaps": true
        },
    ]
}