  "name": "fablab-order-management",
  "version": "0.8.1",
  "description": "An order-management for the fablab connected to an IBM Watson IoT Platform to connect their machinery to this order and planning process",
  "scripts": {
    "apidocs": "shx rm -rf docs/api && apidoc -i api -o docs/api",
    "deploy-staging": "ng build --configuration=staging && npm run api-build && cp appspec.yml dist/ && mkdir -p dist/scripts && cp *.sh dist/scripts",
    "deploy-prod": "ng build --configuration=production && npm run api-build && cp appspec.yml dist/ && mkdir -p dist/scripts && cp *.sh dist/scripts",
    "build": "ng build && npm run api-build",
    "test": "npm run ng-test && npm run api-test",
    "test-local": "npm run ng-test && npm run api-test-local",
    "serve": "concurrently --kill-others \"npm run api-serve\" \"npm run ng-serve\"",
    "dev": "concurrently --kill-others \"npm run api-dev\" \"npm run ng-dev\"",
    "dev-network": "concurrently --kill-others \"npm run api-dev\" \"npm run ng-dev-network\"",
    "lint": "ng lint && eslint api --fix --ext ts",
    "e2e": "ng e2e",
    "api-assets": "shx cp -r api/assets/* dist/assets",
    "api-serve-dev": "cross-env NODE_ENV='dev' node --inspect-brk=9229 dist",
    "api-serve": "node dist",
    "api-build": "tsc && npm run api-assets && npm run lint && shx rm -rf dist/config/email-templates && shx cp -r api/config/email-templates dist/config/email-templates",
    "api-dev": "nodemon -e ts --exec \"npm run api-build && npm run api-serve-dev\"",
    "api-test": "tsc && npm run api-assets && cross-env NODE_ENV='test' node dist/specs/global.spec.js || false ",
    "api-test-local": "tsc && npm run api-assets && cross-env NODE_ENV='testLocal' PORT='65534' node dist/specs/global.spec.js || false ",
    "api-test-dev": "tsc && npm run api-assets && cross-env NODE_ENV='testLocal' node --inspect-brk=9229 dist/specs/global.spec.js",
    "ng-serve": "ng serve --port 80",
    "ng-dev": "ng lint && ng serve --poll=2000",
    "ng-dev-network": "ng lint && ng serve --poll=2000 --host 0.0.0.0",
    "ng-test": "ng test --watch=false --source-map=false"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run apidocs"
    }
  },
