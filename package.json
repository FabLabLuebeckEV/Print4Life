{
  "apidoc": {
    "name": "fablab-order-management",
    "version": "1.0.0",
    "description": "An order-management for the fablab connected to an IBM Watson IoT Platform to connect their machinery to this order and planning process",
    "apidoc": {
      "title": "Fablab Order-Management API",
      "url": "http://localhost:3000/"
    }
  },
  "name": "fablab-order-management",
  "version": "0.6.2",
  "description": "An order-management for the fablab connected to an IBM Watson IoT Platform to connect their machinery to this order and planning process",
  "scripts": {
    "precommit": "npm run build && npm run test-local && npm run apidocs",
    "precommit-prod": "npm run deploy-staging && npm run test-local && npm run apidocs",
    "apidocs": "shx rm -rf docs/api && apidoc -i api -o docs/api",
    "deploy-staging": "ng build --configuration=staging && npm run api-build",
    "deploy-prod": "ng build --configuration=production && npm run api-build",
    "build": "ng build && npm run api-build",
    "test": "npm run ng-test && npm run api-test",
    "test-local": "npm run ng-test && npm run api-test-local",
    "serve": "concurrently --kill-others \"npm run api-serve\" \"npm run ng-serve\"",
    "dev": "concurrently --kill-others \"npm run api-dev\" \"npm run ng-dev\"",
    "lint": "ng lint && eslint api --fix --ext ts",
    "e2e": "ng e2e",
    "api-serve-dev": "cross-env NODE_ENV='dev' node --inspect-brk=9229 dist",
    "api-serve": "node dist",
    "api-build": "tsc && npm run lint && shx rm -rf dist/config/email-templates && shx cp -r api/config/email-templates dist/config/email-templates",
    "api-dev": "nodemon -e ts --exec \"npm run api-build && npm run api-serve-dev\"",
    "api-test": "tsc && cross-env NODE_ENV='test' node dist/specs/global.spec.js || false ",
    "api-test-local": "tsc && cross-env NODE_ENV='testLocal' PORT='65534' node dist/specs/global.spec.js || false ",
    "api-test-dev": "tsc && cross-env NODE_ENV='testLocal' node --inspect-brk=9229 dist/specs/global.spec.js",
    "ng-serve": "ng serve --port 80",
    "ng-dev": "ng lint && ng serve --poll=2000",
    "ng-test": "ng test --watch=false"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run precommit"
    }
  },
  "keywords": [],
