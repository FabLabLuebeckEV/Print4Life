{
  "apidoc": {
    "name": "fablab-order-management",
    "version": "1.0.0",
    "description": "An order-management for the fablab connected to an IBM Watson IoT Platform to connect their machinery to this order and planning process",
    "apidoc": {
      "title": "Fablab Order-Management API",
      "url": "http://localhost:3000/"
    }
  },
  "name": "fablab-order-management",
  "version": "0.2.0",
  "description": "An order-management for the fablab connected to an IBM Watson IoT Platform to connect their machinery to this order and planning process",
  "scripts": {
    "precommit": "npm run build && npm run test-local && npm run apidocs",
    "precommit-prod": "npm run deploy && npm run test-local && npm run apidocs",
    "apidocs": "apidoc -i api -o docs/api",
    "deploy": "ng build --prod && tsc && npm run lint",
    "build": "ng build && tsc && tsc && npm run lint",
    "test": "npm run ng-test && npm run api-test",
    "test-local": "npm run ng-test && npm run api-test-local",
    "serve": "concurrently --kill-others \"npm run api-serve\" \"npm run ng-serve\"",
    "dev": "concurrently --kill-others \"npm run api-dev\" \"npm run ng-dev\"",
    "lint": "ng lint && eslint api --fix --ext ts",
    "e2e": "ng e2e",
    "api-serve-dev": "NODE_ENV='dev' node --inspect-brk=9229 dist",
    "api-serve": "NODE_ENV='prod' node dist",
    "api-build": "tsc && npm run lint",
    "api-dev": "nodemon -e ts --exec \"npm run api-build && npm run api-serve-dev\"",
    "api-test": "tsc && node dist/specs/global.spec.js || false ",
    "api-test-local": "tsc && NODE_ENV='testLocal' node dist/specs/global.spec.js || false ",
    "api-test-dev": "tsc && NODE_ENV='testLocal' node --inspect-brk=9229 dist/specs/global.spec.js",
    "ng-serve": "ng serve --port 80",
    "ng-dev": "ng lint && ng serve --poll=2000",
    "ng-test": "ng test --watch=false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "keywords": [],
  "contributors": [
