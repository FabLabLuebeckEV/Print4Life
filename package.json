{
  "name": "nodets-skeleton",
  "version": "1.0.0",
  "description": "A NodeTS, Mongoose, Express Skeleton",
  "scripts": {
    "build": "tsc && npm run lint && node --inspect-brk=9229 dist",
    "dev": "nodemon -e ts  --exec \"npm run build\"",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --fix --ext ts"
  },
  "keywords": [],
  "author": "Rene Kremer",
  "license": "MIT",
  "repository": "https://gitlab.com/hadesrofl/nodets-skeleton.git",
  "dependencies": {
    "@types/node": "7.0.22",
    "express": "^4.15.3",
    "mongoose": "^5.0.15",
    "nodemon": "^1.17.3",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/mocha": "2.2.41",
    "@types/node": "7.0.22",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "mocha": "3.4.2",
    "supertest": "3.0.0",
    "typescript": "2.3.3",
    "typescript-eslint-parser": "3.0.0"
  }
}
