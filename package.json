{
  "name": "nodets-skeleton",
  "version": "1.0.0",
  "description": "A NodeTS, Mongoose, Express Skeleton",
  "scripts": {
    "api-serve": "NODE_ENV='dev' node --inspect-brk=9229 dist",
    "api-build": "tsc && npm run lint",
    "api-dev": "nodemon -e ts  --exec \"npm run api-build && npm run api-serve\"",
    "api-test": "tsc && jasmine dist/**/*.spec.js",
    "ng-serve": "ng serve --port 80",
    "ng-serve-dev": "ng lint && ng serve --poll=2000",
    "ng-test": "ng test --watch=false",
    "build": "ng build && tsc && npm run lint ",
    "test": "npm run ng-test && npm run api-test",
    "lint": "ng lint && eslint api --fix --ext ts",
    "e2e": "ng e2e"
  },
  "keywords": [],
  "author": "Rene Kremer",
  "license": "MIT",
  "repository": "https://gitlab.com/hadesrofl/nodets-skeleton.git",
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@types/node": "~8.9.4",
    "core-js": "^2.5.4",
    "express": "^4.15.3",
    "mongoose": "^5.0.15",
    "nodemon": "^1.17.3",
    "rxjs": "^6.0.0",
    "winston": "^2.4.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/request": "^2.47.1",
    "codelyzer": "~4.2.1",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "request": "^2.87.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "typescript-eslint-parser": "3.0.0"
  }
}
